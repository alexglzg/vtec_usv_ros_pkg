<launch>

    <node name="tracking_asmc" pkg="sensors" type="tracking_asmc" >
    <param name = "k_x" value = "0.05" />
	<param name = "k_y" value = "0.05" />
	<param name = "kmin_x" value = "0.001" />
	<param name = "kmin_y" value = "0.001" />
	<param name = "k2_x" value = "0.001" />
    <param name = "k2_y" value = "0.001" />
	<param name = "mu_x" value = "0.05" />
	<param name = "mu_y" value = "0.05" />
